{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "target": "ESNext",
    "module": "ESNext",
    "moduleDetection": "force",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "plugins": [{"name": "next"}]
  },
  "include": [],
  "exclude": ["node_modules"]
}
